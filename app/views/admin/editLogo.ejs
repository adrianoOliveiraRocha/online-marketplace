<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Área Administrativa</title>
  <link href="/admin/quill/css/quill.snow.css" rel="stylesheet">
  <%-include('admin_includes/index/links.ejs')%>

</head>

<!-- the function updateData() is in header-->
<body
  onload="JavaScript:updateData()"
  class="hold-transition skin-blue sidebar-mini">

<div class="wrapper">

  <%-include('admin_includes/header.ejs')%>

  <!-- Left side column. contains the logo and sidebar -->
  <%-include('admin_includes/left_side_column.ejs')%>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Painel de Controle
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Início</a></li>
        <li class="active">Painel de Controle</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">

      <div class="box">
        <div class="box-header with-border">
          <h3 class="box-title">Logo Marca</h3>
        </div>

        <div class="box-body">
          <div class="row">
            <div class="col-md-6">
              <label for="">Imagem Atual</label>
            </div>
            <div class="col-md-6">
              <img src="/system-images/mylogo.svg"
              alt=" My Logo " width="200px">
            </div>
          </div>


        </div>
        <!-- /.box-body -->
        <div class="box-footer">
        <div class="control-group">
          <div class="controls">
            <div class="btn-group">
              <button type="submit" class="btn btn-primary"
              onclick="">
                Editar
              </button>
              <a class="btn btn-danger" href="/admin">
                Cancelar
              </a>
            </div>
          </div>
        </div>
        </div>
        <!-- /.box-footer-->
      </div>
      <!-- /.box -->



    </section>
    <!-- /.content -->

  </div>
  <!-- /.content-wrapper -->
  <%-include('./admin_includes/footer.ejs')%>

</div>
<!-- ./wrapper -->

<%-include('admin_includes/index/scripts.ejs')%>

<script src="/admin/notifications.js"></script>


<!-- <script src="/admin/quill/main.js"></script> -->

</body>
</html>
