<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Área Administrativa</title>

  <%-include('admin_includes/index/links.ejs')%>

</head>
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

  <%-include('admin_includes/header.ejs')%>
  
  <!-- Left side column. contains the logo and sidebar -->
  <%-include('admin_includes/left_side_column.ejs')%>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        Área de Cliente        
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Início</a></li>
        <li class="active">Área do Cliente</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      
      <div class="row-fluid">
                      
        <% if(typeof msg != "undefined" && msg != ""){ %>
          <div class="alert alert-success alert-dismissible">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <strong>Mensagem!</strong>
            <%=msg%>
          </div>
        <% } %>

        <% if(typeof error != "undefined" && error != ""){ %>
          <div class="alert alert-danger alert-dismissible">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <h2>Ops! Tivemos um problema técnico. Por favor, entre em contato com o desenvolvedor</h2>
            <p>Desculpe o incoveniente :-(</p>
            <br>
            <strong>Error: </strong>
            <%=error%>
          </div>
        <% } %>
          
      </div>

      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Carrinho de Compras</h3>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              
              content here

            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->

          
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->



    </section>
    <!-- /.content -->

  </div>
  <!-- /.content-wrapper -->
  <%-include('./admin_includes/footer.ejs')%>

</div>
<!-- ./wrapper -->

<%-include('admin_includes/index/scripts.ejs')%>
</body>
</html>
