<%
function getUrl(page) {
  if(typeof currentCategory != 'undefined') {
    return `/?page=${page}&idCategory=${currentCategory}`    
  } else {
    return `/?page=${page}`;    
  }
}
%>

<div class="container">
  <ul class="pagination">
    <% for(let page=1; page <= paginator.nPages; page++) {%>
      <% if(page == paginator.page) {%>
      <li class="active"><a href="#"><%=page%></a></li>
      <% } else { %>
      <li><a href=<%=getUrl(page)%>><%=page%></a></li>
      <% } %>
    <% } %>
  </ul>  
</div>